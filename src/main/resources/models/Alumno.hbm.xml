<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.facultad.trabajopersistencia.Alumno" table="alumno">
        <id name="alumnoId" column="alumnoId">
            <generator class="identity" />
        </id>
        <property name="numeroLegajo" column="numeroLegajo" />
        <property name="añoIngreso" column="añoIngreso" />

        <many-to-one name="ciudad" column="ciudadId" class="com.facultad.trabajopersistencia.Ciudad" />
        
        
    </class>
</hibernate-mapping>

