<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <subclass name="com.facultad.trabajopersistencia.Alumno" discriminator-value="alm" extends="com.facultad.trabajopersistencia.Persona">

        <property name="numeroLegajo" column="numeroLegajo" />
        <property name="añoIngreso" column="añoIngreso" />
    </subclass>
</hibernate-mapping>

